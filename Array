#include <iostream>
#include <conio.h>
using namespace std;
char matrik[2][6] = {{'D', 'H', 'I', 'M', 'A', 'S'}, {'A', 'D', 'I', 'T', 'Y', 'A'}};

int main()
{
     int i, j;
     for (i = 0; i <= 1; i++)
     {
          for (j = 0; j <= 5; j++)
          {
               cout << matrik[i][j] << " ";
          }
     }

     return 0;
}
